#include<iostream>
using namespace std;

int main(){
    int age;
    cout<<"Enter the Age:";
    cin>>age;
    try{
        if(age<18){
            throw age;
            
        }
  else{
      cout<<"Age:"<<age<<"\nAPPOROVED"<<endl;
  }
  
    }
catch(int a){
    cout<<"\nERROR:Underagel("<<age<<")"<<endl;
    
}
    
    
}

//OUTPUT
Enter the Age:34
Age:34
APPOROVED










